<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
    <servers/>
    <playerCommands>
        <playerCommand>
            <name>Ban</name>
            <format>glist ban $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Toggle Mute</name>
            <format>mute $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Kick</name>
            <format>tempban $TARGET_NAME 10s Kicked</format>
        </playerCommand>
        <playerCommand>
            <name>Tempban 5m + Reason</name>
            <format>tempban $TARGET_NAME 5m $REASON</format>
        </playerCommand>
        <playerCommand>
            <name>Tempban 5m</name>
            <format>tempban $TARGET_NAME 5m</format>
        </playerCommand>
        <playerCommand>
            <name>Smite</name>
            <format>smite $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Op</name>
            <format>op $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Deop</name>
            <format>deop $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>GTFO + Reason</name>
            <format>gtfo $TARGET_NAME $REASON</format>
        </playerCommand>
        <playerCommand>
            <name>GTFO</name>
            <format>gtfo $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Toggle Freeze</name>
            <format>fr $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Cage</name>
            <format>cage $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Uncage</name>
            <format>cage $TARGET_NAME off</format>
        </playerCommand>
        <playerCommand>
            <name>Doom</name>
            <format>doom $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Creative</name>
            <format>creative $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Survival</name>
            <format>survival $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Seen Name</name>
            <format>seen $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Seen IP</name>
            <format>seen $TARGET_IP</format>
        </playerCommand>
        <playerCommand>
            <name>Clear Nickname</name>
            <format>nick $TARGET_NAME off</format>
        </playerCommand>
    </playerCommands>
    <favoriteButtons>
       <favoriteButton>
           <label>Op All</label>
           <command>opall</command>
       </favoriteButton>
       <favoriteButton>
           <label>De-Op All</label>
           <command>deopall</command>
       </favoriteButton>
       <favoriteButton>
           <label>Cake</label>
           <command>cake</command>
       </favoriteButton>
       <favoriteButton>
           <label>Cookie</label>
           <command>cookie</command>
       </favoriteButton>
       <favoriteButton>
           <label>Entity Wipe</label>
           <command>ew</command>
       </favoriteButton>
       <favoriteButton>
           <label>Mob Purge</label>
           <command>mp</command>
       </favoriteButton>
       <favoriteButton>
           <label>Potion Clear</label>
           <command>potion clearall</command>
       </favoriteButton>
       <favoriteButton>
           <label>Undisguise All</label>
           <command>uall</command>
       </favoriteButton>
       <favoriteButton>
           <label>Clean Nicks</label>
           <command>nc</command>
       </favoriteButton>
       <favoriteButton>
           <label>Global Freeze</label>
           <command>freeze</command>
       </favoriteButton>
       <favoriteButton>
           <label>AdminMode On</label>
           <command>adminmode on</command>
       </favoriteButton>
       <favoriteButton>
           <label>AdminMode Off</label>
           <command>adminmode off</command>
       </favoriteButton>
       <favoriteButton>
           <label>Fluid Spread</label>
           <command>toggle fluidspread</command>
       </favoriteButton>
       <favoriteButton>
           <label>Place Water</label>
           <command>toggle waterplace</command>
       </favoriteButton>
       <favoriteButton>
           <label>Place Lava</label>
           <command>toggle lavaplace</command>
       </favoriteButton>
       <favoriteButton>
           <label>Fire Spread</label>
           <command>toggle firespread</command>
       </favoriteButton>
       <favoriteButton>
           <label>Explosives</label>
           <command>toggle explosives</command>
       </favoriteButton>
    </favoriteButtons>
</configuration>
